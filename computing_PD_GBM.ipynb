  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "#def persistent_diagram(data_array):\n",
    "def persistent_diagram(data_array):\n",
    "        \n",
    "    bw_img1 = (data_array == 4)  #Active tumor (AT)\n",
    "    bw_img2 = np.isin(data_array, [1,2]) #Tumor area except for AT\n",
    "    dt_img1 = distance_transform_edt(bw_img1)\n",
    "    dt_img2 = distance_transform_edt(bw_img2)\n",
    "    dt_img  = dt_img1 - dt_img2\n",
    "    dt_img[ ~np.isin(data_array, [1,2,4]) ] = np.inf\n",
    "       \n",
    "    cubical_complex = gd.CubicalComplex(top_dimensional_cells = dt_img)\n",
    "\n",
    "    pd = cubical_complex.persistence()\n",
    "\n",
    "    return pd, cubical_complex"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "test_tumor_mni = os.listdir(\"./GBM_Linux(FSL)/Test/Tumor_img_mni152\"  )\n",
    "train_tumor_mni = os.listdir(\"./GBM_Linux(FSL)/Train/Tumor_img_mni152\"  )    \n",
    "failure_tumor_mni = os.listdir(\"./GBM_Linux(FSL)/Failure/Tumor_img_mni152\"  )    \n",
    "\n",
    "test_subject = [filename.split('_')[0] for filename in test_tumor_mni]\n",
    "train_subject = [filename.split('_')[0] for filename in train_tumor_mni]\n",
    "failure_subject = [filename.split('_')[0] for filename in failure_tumor_mni]"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Execute this function to retrieve all data by order"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Test"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
    }
   ],
   "source": [
    "# Specify the path of the main directory\n",
    "main_directory_path = \"./GBM_Linux(FSL)/Test/Tumor_img_mni152/\"  # replace with your main directory path\n",
    "save_path = \"./GBM_Python/GBM_tda/Features_AT_NonAT/\"\n",
    "\n",
    "for ss in range(len(test_subject)):  \n",
    "    Per_Dg, Cub_Com = persistent_diagram( nib.load(\"\".join([main_directory_path,test_tumor_mni[ss]])).get_fdata())\n",
    "    Cub_Com = Cub_Com.cofaces_of_persistence_pairs()\n",
    "    first = []\n",
    "    second = []\n",
    "    third = []\n",
    "\n",
    "    first_c = []\n",
    "    second_c = []\n",
    "    third_c = []\n",
    "    \n",
    "    for outer in Per_Dg:\n",
    "               first.append(outer[0])\n",
    "               second.append(outer[1][0])\n",
    "               third.append(outer[1][1])\n",
    "    for dim in (0,1,2):\n",
    "            for outer in Cub_Com[0][2-dim]:\n",
    "                       first_c.append(2-dim)\n",
    "                       second_c.append(outer[0])\n",
    "                       third_c.append(outer[1])\n",
    "    # Creating the DataFrame\n",
    "    df = pd.DataFrame({\n",
    "    'dimension': first,\n",
    "    'birth': second,\n",
    "    'death': third\n",
    "    })    \n",
    "    \n",
    "    df_c = pd.DataFrame({\n",
    "    'dimension': first_c,\n",
    "    'positive': second_c,\n",
    "    'negative': third_c\n",
    "    })\n",
    "    \n",
    "    df.to_csv(  save_path + test_subject[ss] + '_pd.txt', sep=' ', index=False, header=False)\n",
    "    df_c.to_csv(  save_path + test_subject[ss] + '_locs.txt', sep=' ', index=False, header=False)\n",
    "    print(ss)"
   ]
  },
